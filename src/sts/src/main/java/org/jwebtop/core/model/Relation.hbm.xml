<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.jwebtop.core.model.Relation" table="relation" lazy="false">
		<id name="id" column="rid_">
		<generator class="native"/>
		</id>	
		<property name="name" column="name_" type="string" />
		<property name="email" column="email_" type="string" />
		<property name="mark" column="mark_" type="string" />
	</class>
</hibernate-mapping>